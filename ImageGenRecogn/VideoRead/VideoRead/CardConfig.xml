<?xml version="1.0" encoding="GB2312"?>
<Config>
	<CardParam>
		<!--CG 或者 QP-->
		<CardType>QP</CardType>
		<!--
			COMPOSITE_VIDEO		= 0,
			S_VIDEO  			= 1,
			COMPONENT_VIDEO		= 2
		-->
		<VideoSource>0</VideoSource>
		<SourceIndex>1</SourceIndex>
		<!--
			RGB888(彩色)	= 1,
			ALL8BIT(灰阶)	= 5,
		-->
		<VideoFormat>1</VideoFormat>
	</CardParam>
	<!--
		1X1		= 0,
		1X2 	= 1,
		2X1		= 2,
		1X3		= 3,
		3X1		= 4,
		1X4		= 5,
		4X1		= 6,
		2X2		= 7
	-->
	<AssemblyType>0</AssemblyType>
	
	<!--采集图像参数-->
	<Capture>
		<ImageSize width="640" height="480"/>
